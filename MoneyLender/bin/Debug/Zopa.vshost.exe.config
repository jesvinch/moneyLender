<?xml version="1.0" encoding="utf-8" ?>
<configuration>
    <startup> 
        <supportedRuntime version="v4.0" sku=".NETFramework,Version=v4.5.2" />
    </startup>
  <appSettings>
    <add key="MaxValue" value="15000"/>
    <add key="MinValue" value="1000"/>
    <add key="IncrementAmount" value="100"/>

    <add key="NotEnoughCapital" value="Unfortunately, there isn't enough funds in the market right now"/>
